                app:srcCompat="@drawable/ic_launcher"
                android:id="@+id/imageButton8"
                android:background="@android:color/transparent"/>

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_launcher"
                android:id="@+id/imageButton7"
                android:background="@android:color/transparent"/>
        </TableRow>
    </TableLayout>

</RelativeLayout>
